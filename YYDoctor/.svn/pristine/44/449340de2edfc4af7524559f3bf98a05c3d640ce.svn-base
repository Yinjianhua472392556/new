#import "ConsultListCell.h"

@implementation ConsultListCell

- (void)awakeFromNib {
    [super awakeFromNib];
    self.unreadMessageCountLabel.layer.cornerRadius = 8;//圆角设置
    self.unreadMessageCountLabel.layer.masksToBounds = YES;
    self.headImageView.contentMode = UIViewContentModeScaleAspectFit;//自动适应,保持图片宽高比
}


@end
